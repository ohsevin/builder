     <script type = "text/javascript" language = "javascript" >

    $(document).ready(function() {

        (function() {
            //define the animation refresh (frame rendering) with built-in browser timing
            window.requestAnimationFrame=function(){return window.requestAnimationFrame||window.webkitRequestAnimationFrame||window.mozRequestAnimationFrame||window.msRequestAnimationFrame||window.oRequestAnimationFrame||function(f){window.setTimeout(f,40/60)
                }}
        });

        var canvas = document.getElementById("myCanvas");
        wx = 461;
        wy = 426;
        var dimensions = {
            width: wx,
            height: wy
        };
        $('#myCanvas').css(dimensions).prop(dimensions);

        var ctx = canvas.getContext('2d');
        var img0 = new Image();
        var img1 = new Image();
        var img2 = new Image();
        var img3 = new Image();
        //var img3 = new Image();
        //IMPORTANT: Wait for the pict to be loaded
        img0.onload = function(){
            beginAnimation();
        };
        img0.src = 'images/sysBox.png';
        img1.src = 'images/3dCloudAzure.png';
        img2.src = 'images/3dCloudAWS.png';
        img3.src = 'images/3dCloudAny.png';

        var particles = [];
        var particle;
        var lastScroll = 0;

        function Particle(img,x) {
            var W = wx;
            var H = wy;
            this.x = x;
            this.y = H/2 + ((Math.random() * 20) -10) ;

            this.img = img
            this.speed =  ((Math.random() * 10) - 5);

            this.draw = function(img) {
                ctx.drawImage(img,this.x,this.y);
            };
        }

        function renderFrame() {
            requestAnimationFrame(renderFrame);
            var W = wx;
            var H = wy;
            ctx.clearRect(0, 0, W, H);
            ctx.drawImage(img0,0,0);

            var currentScroll = $('#topRow').scrollTop();
            particles.forEach(function(particle) {

                if (currentScroll > lastScroll || currentScroll < lastScroll){
                    particle.y = particle.y + (currentScroll - lastScroll);
                }

                particle.speed = particle.speed - .1; //gravity falls
                particle.speed = particle.speed * 0.99; //friction
                particle.speed = particle.speed > 10 ? 10:particle.speed;
                particle.speed = particle.speed < -10 ? -10:particle.speed;

                particle.y += particle.speed;

                if(particle.y > (wy - 125)){
                    particle.speed = -(particle.speed + (Math.random()*((particle.speed * .2) - (particle.speed * .1))));
                    particle.y =  wy - 125
                }

               if(particle.y < 100){
                   particle.y = 100;
                   particle.speed = -(particle.speed + (Math.random()*((particle.speed * .2) - (particle.speed * .1))));
               }

                //console.log(particle.y);
                particle.draw(particle.img);
            });
            lastScroll = currentScroll;
        }

        function beginAnimation(){
            particle = new Particle(img0, 0);
            particle = new Particle(img3, 150);
            particles.push(particle);
            particle = new Particle(img1,30 );
            particles.push(particle);
            particle = new Particle(img2, 250);
            particles.push(particle);

            renderFrame();
        }

        <!--insertDocReady-->
    });

